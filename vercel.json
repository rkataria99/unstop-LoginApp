{
    "version": 2,
    "builds": [
      {
        "src": "frontend/**/*",
        "use": "@vercel/static"  
      },
      {
        "src": "frontend/api/server.js",
        "use": "@vercel/node"    
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/frontend/api/$1"  
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"      
      }
    ]
  }
  